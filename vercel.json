{
    "version": 2,
    "builds": [
        {
            "src": "/templates/*",
            "use": "@vercel/static"
        },
        {
            "src": "/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/templates/index.html"
        },
        {
            "src": "/out",
            "dest": "/mainFlask.py"
        }
    ]
}